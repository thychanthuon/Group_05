
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>AdminLTE 3 | Dashboard</title>
    @Html.Partial("~/Views/Shared/AdminLTE/_Style.cshtml");
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

      
        <div class="preloader flex-column justify-content-center align-items-center">
            <img class="animation__shake" src="dist/img/AdminLTELogo.png" alt="AdminLTELogo" height="60" width="60">
        </div>

        @Html.Partial("~/Views/Shared/AdminLTE/_TopNavigation.cshtml");
       
        @Html.Partial("~/Views/Shared/AdminLTE//_MainNavigation.cshtml");

        <div class="content-wrapper">  
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0">Dashboard</h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active">Dashboard v1</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
          
            <section class="content">
                @RenderBody()
            </section>
           
        </div>
       
        @Html.Partial("~/Views/Shared/AdminLTE/_Footer.cshtml");
        <aside class="control-sidebar control-sidebar-dark">
            
        </aside>
    </div>
        @Html.Partial("~/Views/Shared/AdminLTE/_Script.cshtml");
</body>
</html>
